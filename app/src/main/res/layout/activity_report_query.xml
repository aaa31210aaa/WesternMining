<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="activity.kq_report.KqReportQuery">

    <include layout="@layout/custom_toolbar_back"></include>

    <android.support.design.widget.TabLayout
        android:id="@+id/report_query_tab"
        style="@style/MyCustomTabLayout"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_below="@+id/toolbar"
        android:background="@color/colorPrimary">

    </android.support.design.widget.TabLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/report_query_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/report_query_tab"
        android:isScrollContainer="true">

    </android.support.v4.view.ViewPager>


</RelativeLayout>
