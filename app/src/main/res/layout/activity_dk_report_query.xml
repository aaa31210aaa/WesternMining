<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="activity.dk_report.DkReportQuery">

    <include layout="@layout/custom_toolbar_back"></include>


    <android.support.v7.widget.CardView
        android:id="@+id/dzkcwcgzlhz_cv"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qb_px_100"
        android:layout_margin="@dimen/qb_px_20"
        android:clickable="true"
        android:orientation="vertical"
        app:cardBackgroundColor="@color/dzkcwcgzlhz_color"
        app:cardCornerRadius="10dp"
        app:cardElevation="10dp"
        app:cardPreventCornerOverlap="false">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="@string/dzkcwcgzlhz"
            android:textColor="@color/white"
            android:textSize="@dimen/dk_report_text_size" />
    </android.support.v7.widget.CardView>


    <android.support.v7.widget.CardView
        android:id="@+id/sctkwcgzlhz_cv"
        android:layout_width="match_parent"
        android:layout_height="@dimen/qb_px_100"
        android:layout_margin="@dimen/qb_px_20"
        android:clickable="true"
        android:orientation="vertical"
        app:cardBackgroundColor="@color/sctkwcgzlhz_color"
        app:cardCornerRadius="10dp"
        app:cardElevation="10dp"
        app:cardPreventCornerOverlap="false">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="@string/sctkwcgzlhz"
            android:textColor="@color/white"
            android:textSize="@dimen/dk_report_text_size" />
    </android.support.v7.widget.CardView>


</LinearLayout>
